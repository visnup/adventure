!!! 5
html
  head
    link( href="/normalize.css", type="text/css", rel="stylesheet" )
    :stylus
      blue = #56a8e1
      green = #238a58

      @font-face
        font-family "04b_03"
        src url('/fonts/04b_03__-webfont.eot')
        src url('/fonts/04b_03__-webfont.eot?iefix') format('eot'), url('/fonts/04b_03__-webfont.woff') format('woff'), url('/fonts/04b_03__-webfont.ttf') format('truetype'), url('/fonts/04b_03__-webfont.svg#webfontiFL0wwqh') format('svg')
        font-weight normal
        font-style normal
        font-size medium

      @-webkit-keyframes float
        from
          background-position 0 -400px
        to
          background-position 2354px -400px
      
      html, body
        height 100%
      body
        font-family sans-serif
        background blue url('/images/clouds.png') 0 -400px repeat
        -webkit-animation-name float
        -webkit-animation-duration 420s
        -webkit-animation-iteration-count infinite
        -webkit-animation-timing-function linear
      #page
        background green url('/images/grass.png') repeat
        position relative
        margin 500px 0 0
        min-height 100%
      .thing
        position absolute
        font-family '04b_03'
        .bubble
          position absolute
          left 60px
          width 294px - 20px - 21px
          max-height 133px - 21px - 42px
          border-width 21px 20px 42px 21px
          padding 5px
          display none
          text-align center
          overflow-y auto
          white-space pre-wrap
          line-height 125%
          &.frame-0
            border-image url('data:image/png;base64,') 21 20 42 21 fill
          &.frame-1
            border-image url('data:image/png;base64,') 21 20 42 21 fill
          &.frame-2
            border-image url('data:image/png;base64,') 21 20 42 21 fill
        ::-webkit-scrollbar
          width 1ex
        ::-webkit-scrollbar-thumb:vertical
          background-color hsla(0, 0%, 0%, 0.2)
        .textarea-container
          overflow hidden
          position relative
          textarea
            position absolute
            left -100px
            width 80px

    script( type="text/javascript", src="modernizr.custom.66516.js" )
  body
    #page.ground
      h1.when
    script( type="text/javascript", src="jquery-1.9.1.min.js" )
    script( type="text/javascript", src="jquery-migrate-1.1.1.min.js" )
    script( type="text/javascript", src="underscore-min.js" )
    script( type="text/javascript", src="jquery.keylisten.js" )
    script( type="text/javascript", src="socket.io/socket.io.js" )
    script( type="text/javascript", src="watchmaker.js" )
